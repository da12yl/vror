<div class="tiptracker">
  <header>
    <h3>
      <span>Tip Tracker</span>
      <div>
        <button id="show_calendar" class="btn btn-default">
          <span class="glyphicon glyphicon-calendar"></span> Week of March 10th
        </button>
      </div>
    </h3>
  </header>
  <% if flash.any? %>
    <div class="well"><%= flash[:notice] %></div>
  <% end %>
  <section class="row">
    <%= render :template => "tips/panels/time_stat_panels" %>
  </section>
  <%= render :template => "tips/panels/highchart_panel" %>
</div>