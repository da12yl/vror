<table class="table table-hover">
  <thead>
    <th>Name</th>
    <th>Email</th>
    <th>Phone</th>
    <th>Hire Date</th>
    <th>Last Sign In</th>
  </thead>
  <tbody>
    <% @users.each do |user| %>
        <tr>
          <td><%= user.name %></td>
          <td><%= user.email %></td>
          <td><%= number_to_phone user.phone, :area_code => true %></td>
          <td><%= user.hiredate %></td>
          <td><%= user.last_sign_in_at %></td>
        </tr>
    <% end %>
  </tbody>
</table>